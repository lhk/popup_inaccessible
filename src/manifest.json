{
    "manifest_version": 2,
    "name": "parcel demo",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./content.ts"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "options_ui": {
        "page": "options_ui/index.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>"
    ]
}